<mvc:View
    controllerName="pjui500001.controller.ViewAppQRScan"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Button text="Scan QR code!" press="onPress"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
